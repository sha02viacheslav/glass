{
  "homepage": "/",
  "name": "estglass-react",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "lint": "eslint -c .eslintrc.js --ext .tsx .",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier-eslint --write \"src/**/*.jsx\" \"src/**/*.js\" \"tests/**/*.test.ts\" \"tests/**/*.test.tsx\" \"./*.js\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --ext .ts,.tsx,.js,.jsx .",
      "prettier-eslint \"src/**/*.tsx\" \"src/**/*.ts\""
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.20.1",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.14",
    "@mui/lab": "^5.0.0-alpha.114",
    "@mui/material": "^5.11.2",
    "@mui/system": "^5.10.14",
    "@mui/x-date-pickers": "^5.0.8",
    "@stripe/react-stripe-js": "^1.16.5",
    "@stripe/stripe-js": "^1.47.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.1.3",
    "date-fns": "^2.29.3",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "getaddress-autocomplete": "^1.1.7",
    "i": "^0.3.7",
    "image-map-resizer": "^1.0.10",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "npm": "^9.6.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-jwt": "^1.1.8",
    "react-loader-spinner": "^5.3.4",
    "react-pdf": "^6.2.2",
    "react-promise-tracker": "^2.1.1",
    "react-scripts": "5.0.1",
    "react-slick": "^0.29.0",
    "react-sticky": "^6.0.3",
    "slick-carousel": "^1.8.1",
    "stripe": "^11.11.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.14.192",
    "@types/node": "^18.15.10",
    "@types/react": "^18.0.29",
    "@types/react-dom": "^18.0.11",
    "@types/react-gtm-module": "^2.0.1",
    "@types/react-helmet": "^6.1.6",
    "@types/react-pdf": "^6.2.0",
    "@types/react-slick": "^0.23.10",
    "@types/react-sticky": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.7",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^7.1.0",
    "react-router-dom": "^6.4.3",
    "typescript": "^4.8.4"
  }
}
